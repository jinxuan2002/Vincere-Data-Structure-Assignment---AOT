<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.attackontitan.Controller.FindingAbilityController">
   <children>
      <Text layoutX="35.0" layoutY="51.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Finding ability: ">
         <font>
            <Font size="18.0" />
         </font>
      </Text>
      <TextField fx:id="abilityTextField" layoutX="165.0" layoutY="32.0" />
      <Button fx:id="findingButton" layoutX="280.0" layoutY="98.0" mnemonicParsing="false" onAction="#find" text="Find" />
      <Label fx:id="abilityErrorLabel" layoutX="331.0" layoutY="33.0" prefHeight="24.0" prefWidth="171.0" text="Label" textFill="#d90a0a" />
      <Label fx:id="resultLabel" layoutX="20.0" layoutY="144.0" prefHeight="233.0" prefWidth="558.0" text="Label">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
      <Button fx:id="backToPreviousButton" layoutX="545.0" layoutY="18.0" mnemonicParsing="false" onAction="#backToPrevious" text="Back" />
      <Text layoutX="95.0" layoutY="85.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Value: ">
         <font>
            <Font size="18.0" />
         </font>
      </Text>
      <TextField fx:id="valueTextField" layoutX="165.0" layoutY="66.0" />
      <Label fx:id="valueErrorLabel" layoutX="331.0" layoutY="66.0" prefHeight="24.0" prefWidth="171.0" text="Label" textFill="#d90a0a" />
   </children>
</AnchorPane>
